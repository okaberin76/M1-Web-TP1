{
    "parser": "@babel/eslint-parser",
    "extends": "eslint:recommended",
    "env": {
      "browser": true,
      "commonjs": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "strict": ["error", "global"],
      "curly": "warn",
      "semi": "warn",
      "no-unused-vars": "warn"
    }
}